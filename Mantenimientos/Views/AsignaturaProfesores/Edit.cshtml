@model Mantenimientos.Models.AsignaturaProfesores

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar</h1>

<h4>Asignatura Profesores</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="asignaturaProfesoresID" />
            <div class="form-group">
                <label asp-for="AsignaturaID" class="control-label"></label>
                <select asp-for="AsignaturaID" class="form-control" asp-items="ViewBag.AsignaturaID"></select>
                <span asp-validation-for="AsignaturaID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProfesoresID" class="control-label"></label>
                <select asp-for="ProfesoresID" class="form-control" asp-items="ViewBag.ProfesoresID"></select>
                <span asp-validation-for="ProfesoresID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
