@model Mantenimientos.Models.AsignaturaProfesores

@{
    ViewData["Title"] = "Create";
}

<h1>Hola, Docente, elige tus asignaturas</h1>

<h4>Asignaturas Profesores</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AsignaturaID" class="control-label"></label>
                <select asp-for="AsignaturaID" class ="form-control" asp-items="ViewBag.AsignaturaID"></select>
            </div>
            <div class="form-group">
                <label asp-for="ProfesoresID" class="control-label"></label>
                <select asp-for="ProfesoresID" class ="form-control" asp-items="ViewBag.ProfesoresID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
